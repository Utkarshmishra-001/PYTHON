class Solution(object):
    def maxArea(self, height):
        L, R = 0, len(height) - 1
        res = 0
        while L < R:
            w = R - L
            if height[L] < height[R]:
                res = max(res, height[L] * w)
                L += 1
            else:
                res = max(res, height[R] * w)
                R -= 1
        return res
